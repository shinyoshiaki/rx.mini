{
  "name": "rx.mini",
  "version": "1.0.2",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "deploy": "yarn update && yarn publish",
    "test": "npm run type && jest",
    "type": "tsc --noEmit",
    "update": "rm -rf ./lib && npm run build"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "**/*test.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  }
}
